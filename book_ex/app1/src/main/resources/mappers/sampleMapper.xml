<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="org.zerock.persistence.SampleMapper">

	<select id="getUserName" resultType="String">
		select 
		uname
		from tbl_member
		where uid = #{id}
			and upw = #{pw}
	</select>
	
</mapper>